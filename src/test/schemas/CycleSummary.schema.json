    "endDate": { "type": "string" },
    "nCycle": { "type": "number" },
    "cycleLength": { "type": "number" },
    "mensLength": { "type": "number" },
    "ovulationCD": { "type": "number" },
    "ovulationDate": { "type": "string" },
    "spottingDays": { "type": "number" },
    "lhTestPositive": { "type": "number" },
    "lhTestNegative": { "type": "number" },
    "unprotectedSex": { "type": "number" },
    "pregnancyEndDate": { "type": "string" }
