{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "TestKeywords.schema.json",
  "type": "object",
  "properties": {
    "s": {
      "type": "string",
      "minLength": 1,
      "maxLength": 5,
      "pattern": "A[a-z]{1,4}"
    },
    "n": {
      "type": "integer",
      "multipleOf": 2,
      "minimum": 6,
      "maximum": 8
    },
    "dateMap": {
      "type": "object",
      "additionalProperties": false,
      "required": [],
      "patternProperties": {
        ".*": {
          "type": "string"
        }
      },
      "propertyNames": {
        "type": "string",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "minProperties": 1,
      "maxProperties": 3
    },
    "a": {
      "type": "array",
      "items": {
        "type": "number"
      },
      "minItems": 1,
      "maxItems": 3,
      "uniqueItems": true
    }
  },
  "required": [],
  "additionalProperties": false
}
